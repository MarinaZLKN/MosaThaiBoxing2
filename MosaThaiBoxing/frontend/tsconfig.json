{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES6",
    "module": "ES6",
    "strict": true,
    "jsx": "react",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "@images/*": ["src/images/*"]
    },
    "esModuleInterop": true
  },
  "include":  ["src/components", "src/custom.d.ts"],
  "exclude": [
    "node_modules"
  ]
}
